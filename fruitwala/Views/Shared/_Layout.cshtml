                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>*@

        <div class="top-header-area" id="sticker">
		<div class="container">
			<div class="row">
				<div class="col-lg-12 col-sm-12 text-center">
					<div class="main-menu-wrap">
						<!-- logo -->
						<div class="site-logo">
							<a href="/">
								<img src="~/img/logo.png" alt="">
							</a>
						</div>
						<!-- logo -->

						<!-- menu start -->
						<nav class="main-menu">
							<ul>
								<li class="current-list-item"><a href="/">Home</a>
								</li>
								<li><a class="nav-link" asp-area="" asp-controller="Home" asp-action="About">About </a></li>
								</li>
								<li><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Contact">Contact</a></li>
								<li><a href="/Foods">Shop</a>
								</li>
								<li>
									<partial name="_LoginPartial" />
								</li>
								<li>
									<div class="header-icons">
										<a class="shopping-cart" href="/Cart"><i class="fa fa-shopping-cart"></i></a>
									</div>
								</li>
							</ul>
						</nav>
						<div class="mobile-menu"></div>
						<!-- menu end -->
					</div>
				</div>
			</div>
		</div>
	</div>
	
	<!-- search area -->
	@*<div class="search-area">
		<div class="container">
			<div class="row">
				<div class="col-lg-12">
